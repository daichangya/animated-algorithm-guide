{
  "builds": [
    {
      "src": "mkdocs.yml",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "python3 -m pip install -r requirements.txt",
        "buildCommand": "mkdocs build",
        "outputDirectory": "site"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/site/$1"
    }
  ]
}